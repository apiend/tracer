{
  "name": "@diogoxiang/tracer",
  "version": "2.0.3",
  "description": "JavaScript APM Tracer , 前端监控SDK",
  "main": "dist/tracer.js",
  "umd:main": "dist/tracer.umd.js",
  "module": "dist/tracer.module.js",
  "typings": "dist/index.d.ts",
  "keywords": [
    "error",
    "pv",
    "perf",
    "behavior",
    "frontend-tracker"
  ],
  "scripts": {
    "start": "cross-env TARGET=umd-dev rollup -c -w",
    "dev": "rollup -c -w",
    "build": "rollup -c --environment INCLUDE_DEPS,BUILD:production"
  },
  "files": [
    "package.json",
    "dist/",
    "example",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apiend/tracer.git"
  },
  "author": "diogoxiang@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/apiend/tracer/issues"
  },
  "homepage": "https://github.com/apiend/tracer#readme",
  "devDependencies": {
    "@rollup/plugin-json": "^4.0.3",
    "@types/node": "^12.0.2",
    "cross-env": "^5.2.0",
    "husky": "^2.7.0",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2",
    "rollup": "^1.12.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript2": "^0.21.0",
    "typescript": "^3.6.2"
  },
  "dependencies": {
    "tslib": "^1.9.3",
    "ua-device": "^0.1.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ],
    "*.ts": [
      "prettier --parser=typescript --write",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
